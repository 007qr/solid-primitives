{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",
    "newLine": "LF",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "types": ["solid-js"]
  },
  "exclude": ["node_modules"]
}
