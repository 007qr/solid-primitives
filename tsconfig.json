{
  "compilerOptions": {
    "emitDeclarationOnly": true,
    "declaration": true,
    "target": "esnext",
    "newLine": "LF",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "strictNullChecks": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
  },
  "exclude": [
    "node_modules"
  ]
}