{
  "name": "@solid-primitives/template-primitive",
  "version": "0.0.100",
  "description": "A template primitive example.",
  "author": "Your Name <you@youremail.com>",
  "contributors": [],
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/template-primitive#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "bugs": {
    "url": "https://github.com/solidjs-community/solid-primitives/issues"
  },
  "primitive": {
    "name": "template-primitive",
    "stage": 0,
    "list": [
      "createPrimitiveTemplate"
    ],
    "category": "Display & Media"
  },
  "private": false,
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/server.cjs",
  "module": "./dist/server.js",
  "browser": {
    "./dist/server.cjs": "./dist/index.cjs",
    "./dist/server.js": "./dist/index.js"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    "worker": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "browser": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "deno": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "node": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "start": "vite serve dev --host",
    "dev": "npm run start",
    "build": "jiti ../../scripts/build.ts --ssr",
    "test": "vitest"
  },
  "keywords": [
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "jsdom": "^20.0.0",
    "prettier": "^2.7.1",
    "solid-js": "^1.5.4",
    "tslib": "^2.4.0",
    "typescript": "^4.8.2",
    "unocss": "^0.45.15",
    "vite": "^3.0.9",
    "vite-plugin-solid": "^2.3.0",
    "vitest": "^0.23.0"
  },
  "peerDependencies": {
    "solid-js": "^1.4.0"
  }
}
