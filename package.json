{
  "name": "solid-primitives",
  "private": true,
  "description": "A collection of high-quality, community contributed building blocks.",
  "main": "index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "author": "David Di Biase <dave.dibiase@gmail.com>",
  "license": "MIT",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "format": "prettier -w \"packages/**/*.{js,ts,json,css,tsx,jsx,md,html}\"",
    "build": "cross-env CI=true turbo run build",
    "test": "cross-env CI=true turbo run test test:ssr",
    "build-test": "cross-env CI=true turbo run build test test:ssr",
    "new-package": "jiti ./scripts/new-package.ts",
    "update-readme": "jiti ./scripts/update-readme.ts",
    "update-deps": "taze -w -r && pnpm i",
    "changeset": "changeset",
    "release": "pnpm build && changeset publish",
    "version-packages": "changeset version && pnpm i",
    "ci:version": "changeset version && pnpm i && git add ."
  },
  "devDependencies": {
    "@changesets/cli": "^2.24.4",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.7.16",
    "cross-env": "^7.0.3",
    "fs-extra": "^10.1.0",
    "jiti": "^1.14.0",
    "json-to-markdown-table": "^1.0.0",
    "prettier": "^2.7.1",
    "solid-js": "^1.6.0",
    "taze": "^0.8.0",
    "tsup": "^6.2.3",
    "turbo": "^1.4.5",
    "type-fest": "^3.1.0",
    "typescript": "^4.8.4",
    "unocss": "^0.45.15",
    "vite": "^3.0.9",
    "vite-plugin-solid": "^2.3.0",
    "vitest": "^0.23.0"
  },
  "packageManager": "pnpm@7.13.0"
}
