{
  "name": "@solid-primitives/composites",
  "version": "1.0.2",
  "description": "Composable reactive helpers built around watching computation changes.",
  "author": "Damian Tarnawski <gthetarnav@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/davedbase/solid-primitives/tree/main/packages/composites",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davedbase/solid-primitives.git"
  },
  "primitive": {
    "name": "composites",
    "stage": 2,
    "list": [
      "createCompositeEffect",
      "createCompositeComputed",
      "createCompositeMemo",
      "createCompositeRenderEffect",
      "createModifier"
    ],
    "category": "Reactivity"
  },
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "node": {
      "import": "./dist/index.js"
    },
    "import": "./dist/index.js"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "clean": "rimraf dist/",
    "build": "tsup",
    "test": "uvu -r solid-register"
  },
  "keywords": [
    "watch",
    "effect",
    "composites",
    "computed",
    "memo",
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "jsdom": "^18.0.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "solid-register": "^0.0.10",
    "tslib": "^2.0.1",
    "typescript": "^4.5.2",
    "uvu": "^0.5.2"
  },
  "peerDependencies": {
    "solid-js": "^1.2.5"
  },
  "dependencies": {
    "@solid-primitives/debounce": "^1.0.3",
    "@solid-primitives/throttle": "^1.0.4",
    "tsup": "^5.10.1"
  }
}
