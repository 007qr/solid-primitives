{
  "name": "@solid-primitives/composites",
  "version": "1.1.1",
  "description": "Composable reactive helpers built around watching computation changes.",
  "author": "Damian Tarnawski <gthetarnav@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/composites",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "primitive": {
    "name": "composites",
    "stage": 0,
    "list": [
      "createCompositeEffect",
      "createCompositeComputed",
      "createCompositeRenderEffect"
    ],
    "category": "Reactivity"
  },
  "private": false,
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build-disabled": "tsup",
    "test-disabled": "uvu -r solid-register"
  },
  "keywords": [
    "watch",
    "effect",
    "composites",
    "computed",
    "memo",
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "jsdom": "^20.0.0",
    "prettier": "^2.7.1",
    "solid-js": "^1.5.4",
    "solid-register": "^0.2.5",
    "tslib": "^2.4.0",
    "tsup": "^6.2.3",
    "typescript": "^4.8.2",
    "uvu": "^0.5.6"
  },
  "peerDependencies": {
    "solid-js": "^1.3.1"
  },
  "dependencies": {
    "@solid-primitives/debounce": "^1.3.0",
    "@solid-primitives/throttle": "^1.2.0"
  }
}
