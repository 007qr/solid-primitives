{
  "name": "@solid-primitives/script-loader",
  "version": "1.0.3",
  "description": "Primitive to load scripts dynamically",
  "author": "Alex Lohr <alex.lohr@logmein.com>",
  "license": "MIT",
  "homepage": "https://github.com/davedbase/solid-primitives/tree/main/packages/script",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davedbase/solid-primitives.git"
  },
  "primitive": {
    "name": "script-loader",
    "stage": 3,
    "list": [
      "createScriptLoader"
    ],
    "category": "Utilities"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/server.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "node": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "clean": "rimraf dist/",
    "build": "tsup",
    "test": "uvu -r solid-register test"
  },
  "keywords": [
    "script",
    "jsonp",
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "jsdom": "18.1.1",
    "prettier": "^2.0.5",
    "solid-register": "0.1.1",
    "solid-testing-library": "^0.2.0",
    "tslib": "^2.3.1",
    "tsup": "^5.10.1",
    "typescript": "^4.4.3",
    "uvu": "^0.5.2"
  },
  "peerDependencies": {
    "solid-js": "1.3.0"
  },
  "jest": {
    "preset": "solid-jest/preset/browser",
    "setupFiles": [
      "./test/setup.ts"
    ]
  }
}