{
  "name": "@solid-primitives/script-loader",
  "version": "1.1.1",
  "description": "Primitive to load scripts dynamically",
  "author": "Alex Lohr <alex.lohr@logmein.com>",
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/script",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "primitive": {
    "name": "script-loader",
    "stage": 3,
    "list": [
      "createScriptLoader"
    ],
    "category": "Utilities"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/server.cjs",
  "module": "./dist/server.js",
  "browser": {
    "./dist/server.cjs": "./dist/index.cjs",
    "./dist/server.js": "./dist/index.js"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    "worker": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "browser": {
      "development": {
        "import": "./dist/dev.js",
        "require": "./dist/dev.cjs"
      },
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "deno": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "node": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "development": {
      "import": "./dist/dev.js",
      "require": "./dist/dev.cjs"
    },
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "clean": "rimraf dist/",
    "build": "jiti ../../scripts/build.ts --ssr --dev",
    "test": "vitest -c ../../configs/vitest.config.ts",
    "test:ssr": "pnpm run test --mode ssr"
  },
  "keywords": [
    "script",
    "jsonp",
    "solid",
    "primitives"
  ],
  "peerDependencies": {
    "solid-js": "^1.5.0"
  }
}
