{
  "name": "@solid-primitives/script-loader",
  "version": "1.1.1",
  "description": "Primitive to load scripts dynamically",
  "author": "Alex Lohr <alex.lohr@logmein.com>",
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/script",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "primitive": {
    "name": "script-loader",
    "stage": 3,
    "list": [
      "createScriptLoader"
    ],
    "category": "Utilities"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/server.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "node": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "clean": "rimraf dist/",
    "build": "jiti ../../scripts/build.ts --ssr --dev",
    "test": "vitest -c ../../configs/vitest.config.ts",
    "test:ssr": "pnpm run test --mode ssr"
  },
  "keywords": [
    "script",
    "jsonp",
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "jsdom": "^20.0.0",
    "prettier": "^2.7.1",
    "solid-js": "^1.5.0",
    "solid-register": "^0.2.5",
    "solid-testing-library": "^0.2.1",
    "tslib": "^2.4.0",
    "tsup": "^6.2.3",
    "typescript": "^4.8.2",
    "uvu": "^0.5.6"
  },
  "peerDependencies": {
    "solid-js": "^1.5.0"
  },
  "jest": {
    "preset": "solid-jest/preset/browser",
    "setupFiles": [
      "./test/setup.ts"
    ]
  }
}
