{
  "name": "@solid-primitives/tween",
  "version": "1.2.0",
  "description": "Primitive that creates tween functions",
  "author": "Ryan Carniato <ryancarniato@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/tween",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "primitive": {
    "name": "tween",
    "stage": 3,
    "list": [
      "createTween"
    ],
    "category": "Animation"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "main": "./dist/server.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "node": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "build": "tsup"
  },
  "keywords": [
    "animate",
    "tween",
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "prettier": "^2.7.1",
    "solid-testing-library": "^0.2.1",
    "tslib": "^2.4.0",
    "tsup": "^6.2.1",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "solid-js": "^1.3.1"
  }
}
