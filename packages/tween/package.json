{
  "name": "@solid-primitives/tween",
  "version": "1.2.1",
  "description": "Primitive that creates tween functions",
  "author": "Ryan Carniato <ryancarniato@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/tween",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "primitive": {
    "name": "tween",
    "stage": 3,
    "list": [
      "createTween"
    ],
    "category": "Animation"
  },
  "keywords": [
    "animate",
    "tween",
    "solid",
    "primitives"
  ],
  "files": [
    "dist"
  ],
  "type": "module",
  "private": false,
  "sideEffects": false,
  "main": "./dist/server.cjs",
  "module": "./dist/server.js",
  "types": "./dist/index.d.ts",
  "browser": {
    "./dist/server.cjs": "./dist/index.cjs",
    "./dist/server.js": "./dist/index.js"
  },
  "exports": {
    "worker": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "browser": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "deno": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "node": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "build": "jiti ../../scripts/build.ts --ssr",
    "test": "vitest -c ../../configs/vitest.config.ts",
    "test:ssr": "pnpm run test --mode ssr"
  },
  "peerDependencies": {
    "solid-js": "^1.5.0"
  },
  "devDependencies": {
    "solid-js": "^1.5.0",
    "typescript": "^4.8.2"
  }
}
