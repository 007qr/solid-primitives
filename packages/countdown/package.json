{
  "name": "@solid-primitives/countdown",
  "version": "1.0.3",
  "description": "Primitive to create a countdown timer",
  "author": "David Di Biase <dave.dibiase@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/davedbase/solid-primitives/tree/main/packages/countdown",
  "private": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    "require": "./dist/cjs/index.cjs",
    "import": "./dist/index.js",
    "default": "./dist/index.js"
  },
  "sideEffects": "false",
  "scripts": {
    "prebuild": "npm run clean",
    "clean": "rimraf dist/",
    "build": "tsc && tsc --target es5 --module commonjs --declaration false --outDir ./dist/cjs"
  },
  "keywords": [
    "countdown",
    "timer",
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "@types/jest": "^27.0.0",
    "prettier": "^2.0.5",
    "solid-testing-library": "^0.2.0",
    "tslib": "^2.0.1",
    "typescript": "^4.0.2"
  },
  "peerDependencies": {
    "@solid-primitives/timer": "^1.0.2",
    "solid-js": "^1.2.5"
  },
  "jest": {
    "setupFiles": [
      "./test/setup.ts"
    ]
  }
}
