{
  "name": "@solid-primitives/upload",
  "version": "0.0.103",
  "description": "Primitives for uploading files.",
  "author": "Rustam Ashurmatov <rr.ashurmatov.21@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/upload",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "primitive": {
    "name": "upload",
    "stage": 0,
    "list": [
      "createFileUploader",
      "createDropzone"
    ],
    "category": "Browser APIs"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "vite serve dev --host",
    "dev": "yarn start",
    "build": "jiti ../../scripts/build.ts --ssr --dev",
    "test": "vitest -c ../../configs/vitest.config.ts",
    "test:ssr": "pnpm run test --mode ssr"
  },
  "keywords": [
    "file",
    "file upload",
    "upload",
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "jsdom": "^20.0.0",
    "prettier": "^2.7.1",
    "solid-js": "^1.5.0",
    "solid-register": "^0.2.5",
    "tslib": "^2.4.0",
    "tsup": "^6.2.3",
    "typescript": "^4.8.2",
    "unocss": "^0.45.15",
    "uvu": "^0.5.6",
    "vite": "^3.0.9",
    "vite-plugin-solid": "^2.3.0"
  },
  "peerDependencies": {
    "solid-js": "^1.5.0"
  },
  "dependencies": {
    "@solid-primitives/utils": "^3.0.2"
  }
}
