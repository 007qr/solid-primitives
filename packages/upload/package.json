{
  "name": "@solid-primitives/upload",
  "version": "0.0.100",
  "description": "Primitives for uploading files.",
  "author": "Rustam Ashurmatov <rr.ashurmatov.21@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/upload",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "primitive": {
    "name": "upload",
    "stage": 0,
    "list": [
      "createFileUploader",
      "createDropzone"
    ],
    "category": "Browser APIs"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "vite serve dev --host",
    "dev": "yarn start",
    "build": "tsup",
    "test": "uvu -r solid-register"
  },
  "keywords": [
    "file",
    "file upload",
    "upload",
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "jsdom": "^19.0.0",
    "prettier": "^2.6.1",
    "solid-register": "^0.2.5",
    "tslib": "^2.3.1",
    "tsup": "^5.12.2",
    "typescript": "^4.6.3",
    "unocss": "0.30.8",
    "uvu": "^0.5.3",
    "vite": "2.9.1",
    "vite-plugin-solid": "2.2.6"
  },
  "peerDependencies": {
    "solid-js": "^1.3.1"
  },
  "dependencies": {
    "@solid-primitives/utils": "^1.4.0"
  }
}
