{
  "name": "@solid-primitives/mutation-observer",
  "version": "1.1.0",
  "description": "Primitive providing the ability to watch for changes made to the DOM tree.",
  "author": "Damian Tarnawski @thetarnav <gthetarnav@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/mutation-observer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "primitive": {
    "name": "mutation-observer",
    "stage": 3,
    "list": [
      "createMutationObserver"
    ],
    "category": "Browser APIs"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "vite -r ./dev/ -c ./dev/vite.config.ts",
    "build": "tsup",
    "test": "uvu -r solid-register"
  },
  "keywords": [
    "mutationObserver",
    "DOM",
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "@solid-primitives/composites": "^1.1.0",
    "jsdom": "^19.0.0",
    "prettier": "^2.7.1",
    "solid-register": "^0.2.5",
    "tslib": "^2.4.0",
    "tsup": "^6.2.1",
    "unocss": "^0.44.7",
    "uvu": "^0.5.6",
    "vite": "^3.0.4",
    "vite-plugin-solid": "^2.3.0"
  },
  "peerDependencies": {
    "solid-js": "^1.3.0"
  },
  "dependencies": {
    "@solid-primitives/utils": "^1.5.2"
  }
}
