{
  "name": "@solid-primitives/mutation-observer",
  "version": "1.0.3",
  "description": "Primitive providing the ability to watch for changes made to the DOM tree.",
  "author": "Damian Tarnawski @thetarnav <gthetarnav@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/davedbase/solid-primitives/tree/main/packages/mutation-observer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davedbase/solid-primitives.git"
  },
  "primitive": {
    "name": "mutation-observer",
    "stage": 3,
    "list": [
      "createMutationObserver"
    ],
    "category": "Browser APIs"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "vite -r ./dev/ -c ./dev/vite.config.ts",
    "build": "tsup",
    "test": "uvu -r solid-register"
  },
  "keywords": [
    "mutationObserver",
    "DOM",
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "@solid-primitives/composites": "^1.0.1",
    "jsdom": "18.1.1",
    "prettier": "^2.4.1",
    "solid-register": "0.1.1",
    "tslib": "^2.3.1",
    "tsup": "^5.10.1",
    "unocss": "^0.12.4",
    "uvu": "^0.5.2",
    "vite": "2.6.14",
    "vite-plugin-solid": "2.1.2"
  },
  "peerDependencies": {
    "solid-js": "^1.2.5"
  },
  "dependencies": {
    "@solid-primitives/utils": "^0.0.200"
  }
}
