{
  "name": "@solid-primitives/page-visibility",
  "version": "2.0.0",
  "description": "Primitive to track page visibility",
  "author": "David Di Biase",
  "contributors": [
    "Damian Tarnawski <gthetarnav@gmail.com>"
  ],
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/page-visibility",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "primitive": {
    "name": "page-visibility",
    "stage": 3,
    "list": [
      "createPageVisibility"
    ],
    "category": "Display & Media"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/server.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "node": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "start": "vite serve dev --host",
    "dev": "npm run start",
    "build": "tsup",
    "test": "uvu -r solid-register",
    "test:watch": "watchlist src test -- npm test"
  },
  "keywords": [
    "page",
    "visibility",
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "jsdom": "^19.0.0",
    "prettier": "^2.7.1",
    "solid-register": "^0.2.5",
    "tslib": "^2.4.0",
    "tsup": "^6.1.2",
    "typescript": "^4.7.4",
    "unocss": "^0.39.3",
    "uvu": "^0.5.4",
    "vite": "^2.9.12",
    "vite-plugin-solid": "2.2.6",
    "watchlist": "^0.3.1"
  },
  "peerDependencies": {
    "solid-js": "^1.4.3"
  },
  "dependencies": {
    "@solid-primitives/rootless": "^1.1.0"
  }
}
