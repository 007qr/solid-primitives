{
  "name": "@solid-primitives/page-visibility",
  "version": "2.0.8",
  "description": "Primitive to track page visibility",
  "author": "David Di Biase",
  "contributors": [
    "Damian Tarnawski <gthetarnav@gmail.com>",
    "Tom Pichaud <dev.tompichaud@icloud.com>"
  ],
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/page-visibility",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "primitive": {
    "name": "page-visibility",
    "stage": 3,
    "list": [
      "createPageVisibility"
    ],
    "category": "Display & Media"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "browser": {},
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "build": "jiti ../../scripts/build.ts",
    "test": "vitest -c ../../configs/vitest.config.ts",
    "test:ssr": "pnpm run test --mode ssr",
    "test:watch": "watchlist src test -- npm test"
  },
  "keywords": [
    "page",
    "visibility",
    "solid",
    "primitives"
  ],
  "dependencies": {
    "@solid-primitives/event-listener": "workspace:^2.2.9",
    "@solid-primitives/rootless": "workspace:^1.3.1",
    "@solid-primitives/utils": "workspace:^5.5.1"
  },
  "peerDependencies": {
    "solid-js": "^1.6.12"
  },
  "typesVersions": {}
}
