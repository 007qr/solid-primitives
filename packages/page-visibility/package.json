{
  "name": "@solid-primitives/page-visibility",
  "version": "2.0.1",
  "description": "Primitive to track page visibility",
  "author": "David Di Biase",
  "contributors": [
    "Damian Tarnawski <gthetarnav@gmail.com>"
  ],
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/page-visibility",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "primitive": {
    "name": "page-visibility",
    "stage": 3,
    "list": [
      "createPageVisibility"
    ],
    "category": "Display & Media"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/server.cjs",
  "module": "./dist/server.js",
  "browser": {
    "./dist/server.cjs": "./dist/index.cjs",
    "./dist/server.js": "./dist/index.js"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    "worker": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "browser": {
      "development": {
        "import": "./dist/dev.js",
        "require": "./dist/dev.cjs"
      },
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "deno": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "node": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "development": {
      "import": "./dist/dev.js",
      "require": "./dist/dev.cjs"
    },
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "start": "vite serve dev --host",
    "dev": "npm run start",
    "build": "jiti ../../scripts/build.ts --ssr --dev",
    "test": "vitest -c ../../configs/vitest.config.ts",
    "test:ssr": "pnpm run test --mode ssr",
    "test:watch": "watchlist src test -- npm test"
  },
  "keywords": [
    "page",
    "visibility",
    "solid",
    "primitives"
  ],
  "peerDependencies": {
    "solid-js": "^1.5.0"
  },
  "dependencies": {
    "@solid-primitives/rootless": "workspace:^1.1.3"
  },
  "devDependencies": {
    "solid-js": "^1.5.0",
    "typescript": "^4.8.2"
  }
}
