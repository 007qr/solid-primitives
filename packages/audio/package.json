{
  "name": "@solid-primitives/audio",
  "version": "1.3.1",
  "description": "Primitives to manage audio and single sounds.",
  "author": "David Di Biase <dave.dibiase@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/audio",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "primitive": {
    "name": "audio",
    "stage": 3,
    "list": [
      "makeAudio",
      "makeAudioPlayer",
      "createAudio"
    ],
    "category": "Display & Media"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/server.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "node": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "start": "vite serve dev",
    "dev": "vite serve dev",
    "build": "tsup",
    "test": "uvu -b -r solid-register"
  },
  "keywords": [
    "audio",
    "solid",
    "primitives"
  ],
  "testEnvironment": "jsdom",
  "devDependencies": {
    "jsdom": "^20.0.0",
    "prettier": "^2.7.1",
    "solid-heroicons": "^2.0.3",
    "solid-js": "1.5.0-beta.3",
    "solid-register": "^0.2.5",
    "solid-testing-library": "^0.2.1",
    "tslib": "^2.4.0",
    "tsup": "^6.2.2",
    "typescript": "^4.7.4",
    "unocss": "^0.44.7",
    "uvu": "^0.5.6",
    "vite": "^3.0.7",
    "vite-plugin-solid": "^2.3.0"
  },
  "peerDependencies": {
    "solid-js": "^1.4.4"
  },
  "dependencies": {
    "@solid-primitives/utils": "^3.0.1"
  }
}
