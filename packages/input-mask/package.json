{
  "name": "@solid-primitives/input-mask",
  "version": "0.1.3",
  "description": "input masking event primitive.",
  "author": "Alex Lohr <alex.lohr@logmein.com>",
  "contributors": [],
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/input-mask#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "bugs": {
    "url": "https://github.com/solidjs-community/solid-primitives/issues"
  },
  "primitive": {
    "name": "input-mask",
    "stage": 1,
    "list": [
      "createInputMask"
    ],
    "category": "Inputs"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./dist/server.cjs",
  "module": "./dist/server.js",
  "browser": {
    "./dist/server.cjs": "./dist/index.cjs",
    "./dist/server.js": "./dist/index.js"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    "worker": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "browser": {
      "development": {
        "import": "./dist/dev.js",
        "require": "./dist/dev.cjs"
      },
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "deno": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "node": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "development": {
      "import": "./dist/dev.js",
      "require": "./dist/dev.cjs"
    },
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "vite serve dev --host",
    "dev": "npm run start",
    "build": "jiti ../../scripts/build.ts --ssr --dev",
    "test": "vitest -c ../../configs/vitest.config.ts",
    "test:ssr": "pnpm run test --mode ssr"
  },
  "keywords": [
    "solid",
    "primitives"
  ],
  "peerDependencies": {
    "solid-js": "^1.5.0"
  },
  "peerDependenciesMeta": {
    "solid-js": {
      "optional": true
    }
  }
}
