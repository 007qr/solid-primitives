{
  "name": "@solid-primitives/storage",
  "version": "1.3.2",
  "description": "Primitive that provides reactive wrappers for storage access",
  "author": "Alex Lohr <alex.lohr@logmein.com>",
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/storage",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "primitive": {
    "name": "storage",
    "stage": 3,
    "list": [
      "createStorage",
      "createCookieStorage",
      "createAsyncStorage",
      "createStorageSignal",
      "createLocalStorage",
      "createSessionStorage"
    ],
    "category": "Browser APIs"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "test": "uvu -r solid-register"
  },
  "keywords": [
    "localStorage",
    "sessionStorage",
    "cookies",
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "jsdom": "^20.0.0",
    "prettier": "^2.7.1",
    "solid-js": "^1.5.0",
    "solid-register": "^0.2.5",
    "tsup": "^6.2.3",
    "typescript": "^4.8.2",
    "uvu": "^0.5.6"
  },
  "peerDependencies": {
    "solid-js": "^1.5.0"
  }
}
