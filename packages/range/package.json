{
  "name": "@solid-primitives/range",
  "version": "0.1.2",
  "description": "Control Flow Primitives for displaying given number or a number range of elements.",
  "author": "Damian Tarnawski @thetarnav <gthetarnav@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/range#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "bugs": {
    "url": "https://github.com/solidjs-community/solid-primitives/issues"
  },
  "primitive": {
    "name": "range",
    "stage": 1,
    "list": [
      "repeat",
      "mapRange",
      "indexRange",
      "Repeat",
      "Range",
      "IndexRange"
    ],
    "category": "Control Flow"
  },
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "vite serve dev --host",
    "dev": "npm run start",
    "build": "jiti ../../scripts/build.ts --ssr --dev",
    "test": "vitest -c ../../configs/vitest.config.ts",
    "test:ssr": "pnpm run test --mode ssr",
    "test:watch": "vitest watch test"
  },
  "keywords": [
    "control-flow",
    "range",
    "repeat",
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "jsdom": "^20.0.0",
    "prettier": "^2.7.1",
    "solid-js": "^1.5.0",
    "solid-transition-group": "^0.0.8",
    "tslib": "^2.4.0",
    "tsup": "^6.2.3",
    "typescript": "^4.8.2",
    "unocss": "^0.45.15",
    "vite": "^3.0.9",
    "vite-plugin-solid": "^2.3.0",
    "vitest": "^0.23.0"
  },
  "peerDependencies": {
    "solid-js": "^1.5.0"
  },
  "dependencies": {
    "@solid-primitives/utils": "^3.0.2"
  }
}
