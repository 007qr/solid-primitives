{
  "name": "@solid-primitives/scheduled",
  "version": "1.0.0",
  "description": "Primitives for creating scheduled — throttled or debounced — callbacks.",
  "contributors": [
    "David Di Biase <dave.dibiase@gmail.com>",
    "Damian Tarnawski <gthetarnav@gmail.com>"
  ],
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/scheduled#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "bugs": {
    "url": "https://github.com/solidjs-community/solid-primitives/issues"
  },
  "primitive": {
    "name": "scheduled",
    "stage": 2,
    "list": [
      "debounce",
      "throttle",
      "leading"
    ],
    "category": "Utilities"
  },
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "vite serve dev --host",
    "dev": "npm run start",
    "build": "tsup",
    "test": "uvu -r solid-register",
    "test:watch": "watchlist src test -- npm test"
  },
  "keywords": [
    "solid",
    "primitives",
    "scheduled",
    "debounce",
    "throttle"
  ],
  "devDependencies": {
    "@solid-primitives/timer": "^1.3.1",
    "jsdom": "^20.0.0",
    "prettier": "^2.7.1",
    "solid-js": "1.5.0-beta.3",
    "solid-register": "^0.2.5",
    "tslib": "^2.4.0",
    "tsup": "^6.2.2",
    "typescript": "^4.7.4",
    "unocss": "^0.44.7",
    "uvu": "^0.5.6",
    "vite": "^3.0.7",
    "vite-plugin-solid": "^2.3.0",
    "watchlist": "^0.3.1"
  },
  "peerDependencies": {
    "solid-js": "^1.4.0"
  }
}
