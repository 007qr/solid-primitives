{
  "name": "@solid-primitives/media",
  "version": "2.1.6",
  "description": "Primitives for media query and device features",
  "author": "David Di Biase <dave.dibiase@gmail.com>",
  "contributors": [
    {
      "name": "Aditya Agarwal",
      "email": "adityaa803@gmail.com",
      "url": "https://devadi.netlify.app"
    },
    "Damian Tarnawski <gthetarnav@gmail.com>",
    "Tom Pichaud <dev.tompichaud@icloud.com>"
  ],
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/media",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "primitive": {
    "name": "media",
    "stage": 3,
    "list": [
      "makeMediaQueryListener",
      "createMediaQuery",
      "createBreakpoints",
      "sortBreakpoints",
      "usePrefersDark"
    ],
    "category": "Display & Media"
  },
  "keywords": [
    "media",
    "mediaquery",
    "query",
    "breakpoints",
    "responsive",
    "solid",
    "primitives"
  ],
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "dev": "vite serve dev",
    "page": "vite build dev",
    "build": "jiti ../../scripts/build.ts",
    "test": "vitest -c ../../configs/vitest.config.ts",
    "test:ssr": "pnpm run test --mode ssr"
  },
  "dependencies": {
    "@solid-primitives/event-listener": "workspace:^",
    "@solid-primitives/rootless": "workspace:^",
    "@solid-primitives/static-store": "workspace:^",
    "@solid-primitives/utils": "workspace:^"
  },
  "peerDependencies": {
    "solid-js": "^1.6.12"
  },
  "typesVersions": {}
}
