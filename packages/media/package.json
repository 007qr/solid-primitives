{
  "name": "@solid-primitives/media",
  "version": "2.0.2",
  "description": "Primitives for media query and device features",
  "author": "David Di Biase <dave.dibiase@gmail.com>",
  "contributors": [
    {
      "name": "Aditya Agarwal",
      "email": "adityaa803@gmail.com",
      "url": "https://devadi.netlify.app"
    },
    "Damian Tarnawski <gthetarnav@gmail.com>"
  ],
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/media",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "primitive": {
    "name": "media",
    "stage": 3,
    "list": [
      "makeMediaQueryListener",
      "createMediaQuery",
      "createBreakpoints",
      "usePrefersDark"
    ],
    "category": "Display & Media"
  },
  "keywords": [
    "media",
    "mediaquery",
    "query",
    "breakpoints",
    "responsive",
    "solid",
    "primitives"
  ],
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/server.cjs",
  "module": "./dist/server.js",
  "types": "./dist/index.d.ts",
  "browser": {
    "./dist/server.cjs": "./dist/index.cjs",
    "./dist/server.js": "./dist/index.js"
  },
  "exports": {
    "worker": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "browser": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "deno": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "node": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "dev": "npm run start",
    "build": "jiti ../../scripts/build.ts --ssr",
    "test": "vitest -c ../../configs/vitest.config.ts",
    "test:ssr": "pnpm run test --mode ssr"
  },
  "dependencies": {
    "@solid-primitives/event-listener": "workspace:^2.2.2",
    "@solid-primitives/rootless": "workspace:^1.1.3",
    "@solid-primitives/utils": "workspace:^3.0.2"
  },
  "peerDependencies": {
    "solid-js": "^1.5.0"
  },
  "devDependencies": {
    "typescript": "^4.8.2"
  }
}