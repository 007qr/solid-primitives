  "devDependencies": {
    "jsdom": "^19.0.0",
    "prettier": "^2.5.1",
    "solid-register": "^0.2.5",
    "tslib": "^2.3.1",
    "tsup": "^5.12.1",
    "typescript": "^4.5.5",
    "unocss": "0.27.2",
    "uvu": "^0.5.3",
    "vite": "2.8.6",
