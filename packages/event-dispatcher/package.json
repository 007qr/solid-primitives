{
  "name": "@solid-primitives/event-dispatcher",
  "version": "0.0.101",
  "description": "A primitive to dispatch component events.",
  "author": "Aylo Srd <aylo.srd@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/event-dispatcher#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "bugs": {
    "url": "https://github.com/solidjs-community/solid-primitives/issues"
  },
  "primitive": {
    "name": "event-dispatcher",
    "stage": 0,
    "list": [
      "createEventDispatcher"
    ],
    "category": "Utilities"
  },
  "private": false,
  "sideEffects": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/server.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "node": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "start": "vite serve dev --host",
    "dev": "npm run start",
    "build": "tsup",
    "test": "vitest"
  },
  "keywords": [
    "solid",
    "primitives",
    "event",
    "component",
    "custom-event"
  ],
  "devDependencies": {
    "jsdom": "^20.0.0",
    "prettier": "^2.7.1",
    "solid-js": "^1.5.7",
    "tslib": "^2.4.0",
    "tsup": "^6.2.3",
    "typescript": "^4.8.2",
    "unocss": "^0.45.15",
    "vite": "^3.0.9",
    "vite-plugin-solid": "^2.3.0",
    "vitest": "^0.23.0"
  },
  "peerDependencies": {
    "solid-js": "^1.4.0"
  }
}
