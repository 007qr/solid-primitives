{
  "name": "@solid-primitives/geolocation",
  "version": "1.0.4",
  "description": "Primitives to query geolocation and observe changes.",
  "author": "David Di Biase <dave.dibiase@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/davedbase/solid-primitives/tree/main/packages/geolocation",
  "repository": {
    "type": "git",
    "url": "https://github.com/davedbase/solid-primitives/tree/main/packages/geolocation"
  },
  "main": "./dist/server.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/server.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "start": "vite -r ./dev/ -c ./dev/vite.config.ts",
    "build": "tsup",
    "test": "jest"
  },
  "keywords": [
    "geolocation",
    "location",
    "coordinates",
    "primitives"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "prettier": "^2.0.5",
    "solid-testing-library": "^0.2.0",
    "ts-jest": "^27.0.6",
    "tslib": "^2.0.1",
    "tsup": "^5.10.0",
    "@babel/preset-env": "^7.15.8",
    "babel-preset-solid": "^1.1.5",
    "jest": "^27.2.5",
    "wait-for-expect": "^3.0.2",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "solid-js": "^1.0.3"
  },
  "jest": {
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json",
        "babelConfig": {
          "presets": [
            "babel-preset-solid",
            "@babel/preset-env"
          ]
        }
      }
    },
    "setupFiles": [
      "./test/setup.ts"
    ],
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "solid-js/web": "<rootDir>/node_modules/solid-js/web/dist/web.cjs",
      "solid-js": "<rootDir>/node_modules/solid-js/dist/solid.cjs"
    }
  }
}
