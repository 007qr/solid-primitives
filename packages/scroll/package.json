{
  "name": "@solid-primitives/scroll",
  "version": "2.0.0",
  "description": "Reactive primitives to react to element/window scrolling.",
  "author": "David Di Biase <dave.dibiase@gmail.com>",
  "contributors": [
    "Damian Tarnawski <gthetarnav@gmail.com>"
  ],
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/scroll",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "primitive": {
    "name": "scroll",
    "stage": 2,
    "list": [
      "createScrollPosition"
    ],
    "category": "Inputs"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/server.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "node": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "start": "vite serve dev --host",
    "dev": "npm run start",
    "build": "tsup",
    "test": "uvu -r solid-register",
    "test:watch": "watchlist src test -- npm test"
  },
  "keywords": [
    "monitor",
    "scrollTo",
    "scroll",
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "jsdom": "^19.0.0",
    "prettier": "^2.6.2",
    "solid-register": "^0.2.5",
    "tslib": "^2.4.0",
    "tsup": "^5.12.8",
    "typescript": "^4.7.3",
    "unocss": "0.34.0",
    "uvu": "^0.5.3",
    "vite": "2.9.9",
    "vite-plugin-solid": "2.2.6",
    "watchlist": "^0.3.1"
  },
  "dependencies": {
    "@solid-primitives/utils": "^2.1.0",
    "@solid-primitives/event-listener": "^2.1.0"
  },
  "peerDependencies": {
    "solid-js": "^1.4.0"
  }
}
