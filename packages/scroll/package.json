{
  "name": "@solid-primitives/scroll",
  "version": "1.0.4",
  "description": "Primitive to manage scrolling",
  "author": "David Di Biase <dave.dibiase@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/davedbase/solid-primitives/tree/main/packages/scroll",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davedbase/solid-primitives.git"
  },
  "primitive": {
    "name": "scroll",
    "stage": 0,
    "list": [
      "createScrollObserver"
    ],
    "category": "Inputs"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/server.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "node": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "build": "tsup",
    "test": "uvu -r solid-register -r test/setup.ts -i test/setup.ts"
  },
  "keywords": [
    "monitor",
    "scrollTo",
    "scroll",
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "jsdom": "^18.0.1",
    "prettier": "^2.0.5",
    "solid-register": "^0.0.10",
    "tslib": "^2.0.1",
    "tsup": "^5.10.1",
    "typescript": "^4.0.2",
    "uvu": "^0.5.2"
  },
  "peerDependencies": {
    "solid-js": "^1.2.5"
  }
}
