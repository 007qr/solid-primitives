{
  "name": "@solid-primitives/event-props",
  "version": "0.2.1",
  "description": "Primitive to manage events in a reactive way.",
  "author": "Alex Lohr <alex.lohr@logmein.com>",
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/event-props",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "primitive": {
    "name": "event-props",
    "stage": 2,
    "list": [
      "createEventProps"
    ],
    "category": "Browser APIs"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/server.cjs",
  "module": "./dist/server.js",
  "browser": {
    "./dist/server.cjs": "./dist/index.cjs",
    "./dist/server.js": "./dist/index.js"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    "worker": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "browser": {
      "development": {
        "import": "./dist/dev.js",
        "require": "./dist/dev.cjs"
      },
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "deno": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "node": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "development": {
      "import": "./dist/dev.js",
      "require": "./dist/dev.cjs"
    },
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "clean": "rimraf dist/",
    "build": "jiti ../../scripts/build.ts --ssr --dev",
    "typecheck": "tsc --noEmit src/* test/*;",
    "test": "vitest -c ../../configs/vitest.config.ts",
    "test:ssr": "pnpm run test --mode ssr"
  },
  "keywords": [
    "event",
    "store",
    "reactive",
    "solid",
    "primitives"
  ],
  "peerDependencies": {
    "solid-js": "^1.5.0"
  }
}