{
  "name": "@solid-primitives/rootless",
  "version": "1.1.0",
  "description": "A collection of helpers that aim to simplify using reactive primitives outside of reactive roots, and managing disposal of reactive roots.",
  "author": "Damian Tarnawski @thetarnav <gthetarnav@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/rootless#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "primitive": {
    "name": "rootless",
    "stage": 2,
    "list": [
      "createBranch",
      "createCallback",
      "createDisposable",
      "createSharedRoot"
    ],
    "category": "Reactivity"
  },
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "uvu -r solid-register"
  },
  "keywords": [
    "root",
    "solid",
    "primitives"
  ],
  "dependencies": {
    "@solid-primitives/utils": "^2.2.1"
  },
  "devDependencies": {
    "jsdom": "^19.0.0",
    "prettier": "^2.7.1",
    "solid-register": "^0.2.5",
    "tslib": "^2.4.0",
    "tsup": "^6.1.2",
    "typescript": "^4.7.4",
    "uvu": "^0.5.4"
  },
  "peerDependencies": {
    "solid-js": "^1.3.1"
  }
}
