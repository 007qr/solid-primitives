    "template",
    "primitives"
  ],
  "devDependencies": {
    "jsdom": "^18.0.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "solid-register": "^0.0.10",
    "tslib": "^2.0.1",
    "typescript": "^4.5.2",
    "uvu": "^0.5.2"
  },
  "peerDependencies": {
    "solid-js": "^1.2.5"
  }
}
