{
  "name": "@solid-primitives/jsx-tokenizer",
  "version": "0.2.0",
  "description": "A primitive to tokenize your solid-components to enable custom parsing.",
  "author": "Vincent Van Dijck <vandijckv@gmail.com>",
  "contributors": [
    "Damian Tarnawski <gthetarnav@gmail.com>"
  ],
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/jsx-tokenizer#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "bugs": {
    "url": "https://github.com/solidjs-community/solid-primitives/issues"
  },
  "primitive": {
    "name": "jsx-tokenizer",
    "stage": 2,
    "list": [
      "createTokenizer",
      "createToken",
      "resolveTokens",
      "isToken"
    ],
    "category": "Control Flow"
  },
  "keywords": [
    "solid",
    "primitives",
    "jsx"
  ],
  "private": false,
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "development": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/dev.js"
      },
      "require": "./dist/dev.cjs"
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": "./dist/index.cjs"
  },
  "typesVersions": {},
  "scripts": {
    "dev": "vite serve dev",
    "page": "vite build dev",
    "build": "jiti ../../scripts/build.ts --dev",
    "test": "vitest -c ../../configs/vitest.config.ts",
    "test:ssr": "pnpm run test --mode ssr"
  },
  "dependencies": {
    "@solid-primitives/utils": "workspace:^5.4.0"
  },
  "peerDependencies": {
    "solid-js": "^1.6.0"
  }
}
