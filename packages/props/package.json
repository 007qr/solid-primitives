    "solid",
    "primitives"
  ],
  "devDependencies": {
    "esbuild-plugin-solid": "^0.4.1",
    "jsdom": "18.1.1",
    "prettier": "^2.0.5",
    "solid-register": "0.1.5",
    "solid-testing-library": "^0.2.0",
    "tslib": "^2.3.1",
    "tsup": "^5.10.1",
    "typescript": "^4.4.3",
    "uvu": "^0.5.2"
  },
  "peerDependencies": {
    "solid-js": "^1.3.1"
  }
