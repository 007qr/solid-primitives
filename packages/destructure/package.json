{
  "name": "@solid-primitives/destructure",
  "version": "0.1.2",
  "description": "Primitives for destructuring reactive objects – like props or stores – or signals of them into a separate accessors updated individually.",
  "author": "Damian Tarnawski @thetarnav <gthetarnav@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/destructure#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "bugs": {
    "url": "https://github.com/solidjs-community/solid-primitives/issues"
  },
  "primitive": {
    "name": "destructure",
    "stage": 2,
    "list": [
      "destructure"
    ],
    "category": "Reactivity"
  },
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "vite serve dev",
    "dev": "vite serve dev",
    "build": "jiti ../../scripts/build.ts --ssr --dev",
    "test": "vitest -c ../../configs/vitest.config.ts",
    "test:ssr": "pnpm run test --mode ssr"
  },
  "keywords": [
    "props",
    "store",
    "solid",
    "primitives"
  ],
  "dependencies": {
    "@solid-primitives/utils": "workspace:^3.0.2"
  },
  "devDependencies": {
    "jsdom": "^20.0.0",
    "prettier": "^2.7.1",
    "solid-js": "^1.5.0",
    "solid-register": "^0.2.5",
    "tslib": "^2.4.0",
    "tsup": "^6.2.3",
    "typescript": "^4.8.2",
    "unocss": "^0.45.15",
    "uvu": "^0.5.6",
    "vite": "^3.0.9",
    "vite-plugin-solid": "^2.3.0"
  },
  "peerDependencies": {
    "solid-js": "^1.5.0"
  }
}
