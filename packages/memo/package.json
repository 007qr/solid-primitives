{
  "name": "@solid-primitives/memo",
  "version": "1.0.0",
  "description": "Collection of custom memo primitives. They extend Solid's createMemo functionality while keeping the usage similar.",
  "author": "Damian Tarnawski @thetarnav <gthetarnav@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/memo#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "bugs": {
    "url": "https://github.com/solidjs-community/solid-primitives/issues"
  },
  "primitive": {
    "name": "memo",
    "stage": 3,
    "list": [
      "createCurtain",
      "createWritableMemo",
      "createLazyMemo",
      "createAsyncMemo",
      "createDebouncedMemo",
      "createDebouncedMemoOn",
      "createThrottledMemo",
      "createPureReaction",
      "createMemoCache"
    ],
    "category": "Reactivity"
  },
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/server.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "node": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "vite serve dev --host",
    "dev": "npm run start",
    "build": "tsup",
    "test": "uvu -r solid-register"
  },
  "keywords": [
    "solid",
    "primitives",
    "memo"
  ],
  "dependencies": {
    "@solid-primitives/scheduled": "1.0.0",
    "@solid-primitives/utils": "^2.1.1"
  },
  "devDependencies": {
    "@solid-primitives/mouse": "^2.0.0",
    "jsdom": "^19.0.0",
    "prettier": "^2.7.1",
    "solid-app-router": "^0.3.3",
    "solid-register": "^0.2.5",
    "tslib": "^2.4.0",
    "tsup": "^6.1.2",
    "typescript": "^4.7.4",
    "unocss": "^0.39.3",
    "uvu": "^0.5.4",
    "vite": "^2.9.12",
    "vite-plugin-solid": "2.2.6"
  },
  "peerDependencies": {
    "solid-js": "^1.4.2"
  }
}
