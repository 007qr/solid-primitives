{
  "name": "@solid-primitives/date",
  "version": "1.1.0",
  "description": "Collection of reactive primitives and utility functions, providing easier ways to deal with dates in SolidJS",
  "author": "Damian Tarnawski @thetarnav <gthetarnav@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/davedbase/solid-primitives/tree/main/packages/date",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davedbase/solid-primitives.git"
  },
  "primitive": {
    "name": "date",
    "stage": 2,
    "list": [
      "createDate",
      "createDateNow",
      "createTime",
      "createTimeDifference",
      "createTimeDifferenceFromNow",
      "createTimeAgo",
      "createCountdown",
      "createCountdownFromNow"
    ],
    "category": "Utilities"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "vite serve dev",
    "dev": "vite serve dev",
    "build": "tsup",
    "test": "uvu -r solid-register"
  },
  "keywords": [
    "date",
    "time",
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "@solid-primitives/composites": "^1.0.1",
    "@solid-primitives/event-listener": "^1.3.8",
    "date-fns": "^2.26.0",
    "jsdom": "18.1.1",
    "prettier": "^2.4.1",
    "solid-register": "0.0.18",
    "tslib": "^2.3.1",
    "tsup": "^5.10.1",
    "typescript": "^4.5.2",
    "unocss": "0.21.2",
    "uvu": "^0.5.2",
    "vite": "2.7.10",
    "vite-plugin-solid": "2.2.1"
  },
  "peerDependencies": {
    "solid-js": "^1.3.1"
  },
  "dependencies": {
    "@solid-primitives/utils": "^0.1.2"
  }
}