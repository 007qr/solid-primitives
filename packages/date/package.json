{
  "name": "@solid-primitives/date",
  "version": "2.0.4",
  "description": "Collection of reactive primitives and utility functions, providing easier ways to deal with dates in SolidJS",
  "author": "Damian Tarnawski @thetarnav <gthetarnav@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/date",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "primitive": {
    "name": "date",
    "stage": 3,
    "list": [
      "createDate",
      "createDateNow",
      "createTimeDifference",
      "createTimeDifferenceFromNow",
      "createTimeAgo",
      "createCountdown",
      "createCountdownFromNow"
    ],
    "category": "Utilities"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "browser": {},
  "types": "./dist/index.d.ts",
  "exports": {
    "browser": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "start": "vite serve dev",
    "dev": "vite serve dev",
    "build": "jiti ../../scripts/build.ts",
    "test": "vitest -c ../../configs/vitest.config.ts",
    "test:ssr": "pnpm run test --mode ssr"
  },
  "keywords": [
    "date",
    "time",
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "@solid-primitives/event-listener": "^2.2.3",
    "date-fns": "^2.29.2",
    "solid-js": "^1.5.0"
  },
  "dependencies": {
    "@solid-primitives/memo": "^1.1.0",
    "@solid-primitives/timer": "^1.3.4",
    "@solid-primitives/utils": "^3.1.0"
  },
  "peerDependencies": {
    "solid-js": "^1.5.0"
  }
}