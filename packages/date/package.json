  "keywords": [
    "date",
    "time",
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "@solid-primitives/composites": "^1.0.1",
    "@solid-primitives/event-listener": "^1.3.8",
    "date-fns": "^2.26.0",
    "jsdom": "18.1.1",
    "prettier": "^2.4.1",
    "solid-register": "0.1.1",
    "tslib": "^2.3.1",
    "tsup": "^5.10.1",
    "typescript": "^4.5.2",
    "unocss": "^0.12.4",
    "uvu": "^0.5.2",
    "vite": "2.6.14",
    "vite-plugin-solid": "2.1.2"
  },
  "peerDependencies": {
    "solid-js": "^1.3.0-rc.4"
  },
  "dependencies": {
    "@solid-primitives/utils": "^0.1.2"
  }
