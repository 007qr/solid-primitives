{
  "name": "@solid-primitives/devices",
  "version": "1.0.4",
  "description": "Primitive that enumerates media devices",
  "author": "Alex Lohr <alex.lohr@logmein.com>",
  "license": "MIT",
  "homepage": "https://github.com/davedbase/solid-primitives/tree/main/packages/devices",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davedbase/solid-primitives.git"
  },
  "primitive": {
    "name": "devices",
    "stage": 3,
    "list": [
      "createDevices",
      "createMicrophones",
      "createSpeakers",
      "createCameras"
    ],
    "category": "Display & Media"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "node": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "clean": "rimraf dist/",
    "build": "tsup",
    "test": "uvu -r solid-register"
  },
  "keywords": [
    "media",
    "devices",
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "jsdom": "^18.1.1",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "solid-register": "0.1.1",
    "tslib": "2.3.1",
    "tsup": "^5.10.1",
    "typescript": "^4.5.2",
    "uvu": "^0.5.2"
  },
  "peerDependencies": {
    "solid-js": "^1.3.1"
  }
}