{
  "name": "@solid-primitives/mouse",
  "version": "2.0.10",
  "description": "A collection of Solid Primitives, that capture current mouse cursor position, and help to deal with common related usecases.",
  "author": "Damian Tarnawski <gthetarnav@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/mouse#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "primitive": {
    "name": "mouse",
    "stage": 3,
    "list": [
      "createMousePosition",
      "createPositionToElement"
    ],
    "category": "Inputs"
  },
  "sideEffects": false,
  "private": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": "./dist/index.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "vite serve dev --host",
    "dev": "vite serve dev",
    "page": "vite build dev",
    "build": "jiti ../../scripts/build.ts",
    "test": "vitest -c ../../configs/vitest.config.ts",
    "test:ssr": "pnpm run test --mode ssr",
    "test:watch": "watchlist src test -- npm test"
  },
  "keywords": [
    "mouse",
    "touch",
    "cursor",
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "@solid-primitives/raf": "workspace:^2.1.9"
  },
  "dependencies": {
    "@solid-primitives/event-listener": "^2.2.9",
    "@solid-primitives/rootless": "^1.3.1",
    "@solid-primitives/static-store": "workspace:^0.0.1",
    "@solid-primitives/utils": "^5.5.1"
  },
  "peerDependencies": {
    "solid-js": "^1.6.12"
  },
  "typesVersions": {}
}
