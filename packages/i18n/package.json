{
  "name": "@solid-primitives/i18n",
  "version": "1.0.7",
  "description": "Primitive to create and use i18n primitives.",
  "author": "Alexandre Mouton-Brady <amoutonbrady@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/davedbase/solid-primitives/tree/main/packages/i18n",
  "repository": {
    "type": "git",
    "url": "https://github.com/davedbase/solid-primitives/tree/main/packages/i18n"
  },
  "main": "./dist/server.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/server.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "start": "vite -r ./dev/ -c ./dev/vite.config.ts",
    "build": "tsup",
    "test": "jest"
  },
  "keywords": [
    "i18n",
    "internationalization",
    "language",
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.15.8",
    "@types/jest": "^26.0.10",
    "babel-preset-solid": "^1.1.5",
    "jest": "^27.2.5",
    "prettier": "^2.0.5",
    "solid-testing-library": "^0.2.0",
    "ts-jest": "^27.0.5",
    "tsup": "^5.10.0",
    "tslib": "^2.0.1",
    "typescript": "^4.0.2"
  },
  "peerDependencies": {
    "solid-js": "^1.2.5"
  },
  "jest": {
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json",
        "babelConfig": {
          "presets": [
            "babel-preset-solid",
            "@babel/preset-env"
          ]
        }
      }
    },
    "setupFiles": [
      "./test/setup.ts"
    ],
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "solid-js/web": "<rootDir>/node_modules/solid-js/web/dist/web.cjs",
      "solid-js": "<rootDir>/node_modules/solid-js/dist/solid.cjs"
    }
  }
}
