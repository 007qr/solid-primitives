{
  "name": "@solid-primitives/event-bus",
  "version": "0.1.2",
  "description": "A collection of SolidJS primitives providing various features of a pubsub/event-emitter/event-bus.",
  "author": "Damian Tarnawski @thetarnav <gthetarnav@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/event-bus#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "primitive": {
    "name": "event-bus",
    "stage": 2,
    "list": [
      "createSimpleEmitter",
      "createEmitter",
      "createEventBus",
      "createEventHub",
      "createEventStack"
    ],
    "category": "Utilities"
  },
  "private": false,
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "browser": {},
  "types": "./dist/index.d.ts",
  "exports": {
    "browser": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "dev": "vite serve dev",
    "start": "vite serve dev",
    "build": "jiti ../../scripts/build.ts",
    "test": "vitest -c ../../configs/vitest.config.ts",
    "test:ssr": "pnpm run test --mode ssr"
  },
  "keywords": [
    "eventbus",
    "pubsub",
    "event",
    "emitter",
    "solid",
    "primitives"
  ],
  "dependencies": {
    "@solid-primitives/immutable": "^0.1.2",
    "@solid-primitives/utils": "^3.0.2"
  },
  "peerDependencies": {
    "solid-js": "^1.5.0"
  },
  "devDependencies": {
    "solid-js": "^1.5.0",
    "typescript": "^4.8.2"
  }
}
