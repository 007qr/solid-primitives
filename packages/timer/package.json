{
  "name": "@solid-primitives/timer",
  "version": "1.3.1",
  "description": "Primitives to manage timeout and interval",
  "author": "David Di Biase <dave.dibiase@gmail.com>",
  "contributors": [
    "Xavier Loh <xavier.loh.yz@gmail.com>",
    "Damian Tarnawski <gthetarnav@gmail.com>"
  ],
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/timer#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "bugs": {
    "url": "https://github.com/solidjs-community/solid-primitives/issues"
  },
  "primitive": {
    "name": "timer",
    "stage": 3,
    "list": [
      "makeTimer",
      "createTimer",
      "createTimeoutLoop",
      "createPolled",
      "createIntervalCounter"
    ],
    "category": "Browser APIs"
  },
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "vite serve dev --host",
    "dev": "npm run start",
    "build": "tsup",
    "test": "uvu -r solid-register"
  },
  "keywords": [
    "setInterval",
    "setTimeout",
    "timer",
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "prettier": "^2.7.1",
    "solid-register": "^0.2.5",
    "tslib": "^2.4.0",
    "tsup": "^6.1.3",
    "typescript": "^4.7.4",
    "unocss": "^0.44.7",
    "uvu": "^0.5.6",
    "vite": "^3.0.2",
    "vite-plugin-solid": "^2.3.0"
  },
  "peerDependencies": {
    "solid-js": "^1.3.0"
  },
  "solid-register": {
    "dom": false
  }
}
