{
  "name": "@solid-primitives/timer",
  "version": "1.0.0",
  "description": "Primitive to manage timeout and interval",
  "author": "David Di Biase <dave.dibiase@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/davedbase/solid-primitives/tree/main/packages/timer",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/index.js",
    "default": "./dist/index.js"
  },
  "sideEffects": "false",
  "scripts": {
    "prebuild": "npm run clean",
    "clean": "rimraf dist/",
    "build": "tsc && tsc --target es5 --module commonjs --declaration false --outDir ./dist/cjs"
  },
  "keywords": [
    "setInterval",
    "setTimeout",
    "timer",
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "prettier": "^2.0.5",
    "solid-testing-library": "^0.2.0",
    "tslib": "^2.0.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "solid-js": "^1.2.5"
  },
  "jest": {
    "setupFiles": [
      "./test/setup.ts"
    ]
  }
}
