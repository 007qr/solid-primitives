{
  "name": "@solid-primitives/active-element",
  "version": "1.0.2",
  "description": "A reactive document.activeElement. Check which element is currently focused.",
  "author": "Damian Tarnawski @thetarnav <gthetarnav@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/davedbase/solid-primitives/tree/main/packages/active-element#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davedbase/solid-primitives.git"
  },
  "primitive": {
    "name": "active-element",
    "stage": 2,
    "list": [
      "createActiveElement",
      "createIsElementActive"
    ],
    "category": "Inputs"
  },
  "private": false,
  "sideEffects": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/server.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "node": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "start": "vite -r ./dev/ -c ./dev/vite.config.ts",
    "build": "tsup",
    "test": "uvu -r solid-register",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "focus",
    "activeElement",
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "prettier": "^2.5.0",
    "solid-register": "0.1.3",
    "tslib": "^2.3.1",
    "tsup": "^5.10.0",
    "unocss": "0.21.2",
    "uvu": "^0.5.2",
    "vite": "2.7.10",
    "vite-plugin-solid": "2.2.1"
  },
  "peerDependencies": {
    "solid-js": "^1.3.1"
  },
  "dependencies": {
    "@solid-primitives/event-listener": "^1.4.1",
    "@solid-primitives/utils": "^0.1.2"
  }
}
